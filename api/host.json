{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "cors": {
    "allowedOrigins": [
      "http://localhost:3000",
      "http://127.0.0.1:5500",
      "http://localhost:5500"
    ],
    "supportedMethods": [
      "GET",
      "POST",
      "OPTIONS"
    ],
    "supportedHeaders": [
      "*"
    ],
    "exposedHeaders": [],
    "allowCredentials": true,
    "maxAge": 1200
  }
}